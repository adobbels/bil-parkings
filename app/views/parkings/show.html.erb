
    <!--============================= SUBPAGE HEADER BG =============================-->
    <section class="subpage-bg">
        <div class="container-fluid">
            <div class="row justify-content-center">
                <div class="col-md-10">
                    <div class="titile-block title-block_subpage">
                        <h2>Your rentals</h2>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--// SUBPAGE HEADER BG -->
    <!--============================= BLOG 2 =============================-->
    <section class="main-block">
        <div class="container-fluid">
            <div class="row">
                <% @MyBookings.each do |booking| %>
                  <div class="col-md-6 col-lg-3 article-first">
                      <div class="news-block">
                          <div class="news-title">
                              <h5>Rent on <%= booking.created_at.strftime("%m/%d/%Y") %></h5>
                              <p class="blog2-thumbnail-name">By <%= Profile.find(booking.profile_id).first_name %> <%= Profile.find(booking.profile_id).last_name %></p>
                              <p class="blog2-thumbnail-name">For €10.00</p>
                              <%= link_to "← Back to profile", profile_path(@profile), class:"blog2-link" %>
                          </div>
                      </div>
                  </div>
                <% end  %>
            </div>
        </div>
    </section>
    <!--//END BLOG -->
